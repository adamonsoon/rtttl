{
  "name": "rtttl-parse",
  "version": "0.0.0-development",
  "description": "Parse Nokia Ring Tone Text Transfer Language (RTTTL)",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "build": "babel --out-dir dist --ignore *.test.js src",
    "prebuild": "rimraf dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamonsoon/rtttl-parse.git"
  },
  "keywords": [
    "rtttl",
    "nokia"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Adam Rahwane <adam@produckt.co.il> (https://github.com/adamonsoon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamonsoon/rtttl-parse/issues"
  },
  "homepage": "https://github.com/adamonsoon/rtttl-parse#readme",
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-2": "6.22.0",
    "chai": "3.5.0",
    "codecov": "2.1.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "rimraf": "2.6.1",
    "semantic-release": "^6.3.2",
    "sinon": "2.1.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  },
  "babel": {
    "presets": ["es2015", "stage-2"]
  }
}
